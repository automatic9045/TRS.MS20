<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AccentColor" A="255" R="10" G="24" B="106"/>

    <SolidColorBrush x:Key="BackgroundBrush" Color="#C0C0C0"/>
    <SolidColorBrush x:Key="FocusedTextBoxBrush" Color="Aqua"/>
    <SolidColorBrush x:Key="FocusedButtonBrush" Color="#FEFEE8"/>
    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}"/>

    <SolidColorBrush x:Key="BorderBrush" Color="#808080"/>
    <SolidColorBrush x:Key="DarkBorderBrush" Color="#404040"/>

    <SolidColorBrush x:Key="ButtonFocuedBorderBrush" Color="#646464"/>
    <SolidColorBrush x:Key="ButtonShadowBrush" Color="#717171"/>
    <SolidColorBrush x:Key="ButtonBorderLightBrush" Color="#DFDFDF"/>
    <SolidColorBrush x:Key="ButtonBorderShadowBrush" Color="#7F7F7F"/>

</ResourceDictionary>